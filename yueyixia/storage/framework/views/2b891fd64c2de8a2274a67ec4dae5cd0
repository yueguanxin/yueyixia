      <script type="text/javascript">
          $("#p2").hide();
          /*判断购票类型*/
          $(document).ready(function(){
              $("#t1").on('click',function(){
                  $("#p1").show();
                  $("#p2").hide();
              })
          });
          $(document).ready(function(){
              $("#t2").on('click',function(){
                  var val =  parseInt($("#room_number").val());
                  var zhi1 =  parseInt($("#y2").val());
                  var nzhi1 = val*zhi1;
                  $("#pr2").attr("value",nzhi1);
                  $("#p2").show();
                  $("#p1").hide();
              })
          });
          $(document).ready(function(){
              $("#add").on("click",function(){
                  var val =  parseInt($("#room_number").val());
                  var zhi =  parseInt($("#y1").val());
                  var zhi1 =  parseInt($("#y2").val());
                  val+=1;
                  var nzhi = val*zhi;
                  var nzhi1 = val*zhi1;
                  //alert(val);
                  $("#room_number").attr("value",val);
                  $("#sum1").attr("value",val);
                  $("#sum2").attr("value",val);
                  $("#pr1").attr("value",nzhi);
                  $("#pr2").attr("value",nzhi1);
              });
          })

          $(document).ready(function(){
              $("#reduce").on("click",function(){
                  var val =  parseInt($("#room_number").val());
                  var zhi =  parseInt($("#y1").val());
                  var zhi1 =  parseInt($("#y2").val());
                  var pzhi =  parseInt($("#pr1").val());
                  var pzhi2 =  parseInt($("#pr2").val());
                  val=val-1;
                  if(val<1){
                      val=1;
                  }
                  var nzhi = pzhi-zhi;
                  var nzhi1 = pzhi2-zhi1;
                  if(nzhi<zhi){
                      nzhi=zhi;
                  }
                  if(nzhi1<zhi1){
                      nzhi1=zhi1;
                  }
                  //$("#room_number").attr("value",val);
                  //$("#room_number").html(val);
                  $("#sum1").attr("value",val);
                  $("#sum2").attr("value",val);
                  $("#pr1").attr("value",nzhi);
                  $("#pr2").attr("value",nzhi1);
              });
          })
      </script>

