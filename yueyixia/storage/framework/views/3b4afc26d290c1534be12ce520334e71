<script type="text/javascript">
	$(function(){
		$(".money").click(function(){
			var money=$(this).val();
			var vid_all="";
			$("input[name='vid']:checked").each(function(){
                var vid=$(this).val();
				vid_all=vid_all+','+vid;
            });
			//alert(vid_all);
			newvid_all=vid_all.substr(1);
			//alert(newvid_all);
			$.get("/check",{money:money,vid:newvid_all},function(date){
				$(".house-items-list").html(date);
			})
		})

		//收藏
		$(".yicon-heart").click(function(){
			var vid=$(this).attr('value');
			$(this).parent().html('<span class="yicon-heart-red" value="'+vid+'"></span>');
			alert(vid)
			$.get("/collection_add",{vid:vid},function(date){
				alert(date)
			})
		})
		//取消收藏
		$(".yicon-heart-red").click(function(){
			alert("111")
			var vid=$(this).attr('value');
			$(this).parent().html('<span class="yicon-heart" value="'+vid+'"></span>');
			//alert(vid)
		})
	})
</script> 

